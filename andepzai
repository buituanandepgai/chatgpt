loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Rayfield/main/source"))()

local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()
local Player = game.Players.LocalPlayer
local VirtualUser = game:service("VirtualUser")

-- üõ°Ô∏è Anti-AFK
Player.Idled:Connect(function()
    VirtualUser:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
    wait(1)
    VirtualUser:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
end)

-- üîê Key h·ªá th·ªëng
local correctKey = "buituanan"

Rayfield:Prompt({
    Title = "üîë H·ªá Th·ªëng Nh·∫≠p Key",
    SubTitle = "Vui l√≤ng nh·∫≠p key ƒë·ªÉ ti·∫øp t·ª•c",
    PlaceholderText = "Nh·∫≠p key t·∫°i ƒë√¢y...",
    ConfirmText = "X√°c nh·∫≠n",
    InputType = "Default",
    Callback = function(input)
        if input == correctKey then
            Rayfield:Notify({
                Title = "‚úÖ Th√†nh c√¥ng!",
                Content = "Key ch√≠nh x√°c, ƒëang m·ªü script...",
                Duration = 4,
            })

            -- ü™ü Giao di·ªán ch√≠nh
            local Window = Rayfield:CreateWindow({
                Name = "Chat GPT Code",
                LoadingTitle = "Chat GPT Code",
                LoadingSubtitle = "by tuanan",
                ConfigurationSaving = {
                    Enabled = false
                },
                Discord = {
                    Enabled = false
                },
                KeySystem = false
            })

            -- ‚öôÔ∏è Bi·∫øn
            local autoFarm = false
            local autoIsland = false
            local delayDanh = 1

            function GetLevelInfo()
                local level = Player.Data.Level.Value
                if level < 10 then
                    return "Bandit", CFrame.new(1038, 16, 1547)
                elseif level < 30 then
                    return "Monkey", CFrame.new(-1602, 20, 152)
                elseif level < 60 then
                    return "Gorilla", CFrame.new(-1125, 40, -525)
                elseif level < 100 then
                    return "Pirate", CFrame.new(-5105, 5, 430)
                else
                    return "Bandit", CFrame.new(1038, 16, 1547)
                end
            end

            -- ‚öîÔ∏è Auto Farm
            spawn(function()
                while wait() do
                    if autoFarm then
                        local enemyName, _ = GetLevelInfo()
                        for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                            if v.Name:find(enemyName) and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                                repeat wait(delayDanh)
                                    pcall(function()
                                        Player.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,10,0)
                                        game:GetService("VirtualInputManager"):SendKeyEvent(true, "Z", false, game)
                                        wait(0.05)
                                        game:GetService("VirtualInputManager"):SendKeyEvent(false, "Z", false, game)
                                    end)
                                until v.Humanoid.Health <= 0 or not v.Parent or not autoFarm
                            end
                        end
                    end
                end
            end)

            -- üö¢ Auto ƒê·ªïi ƒê·∫£o
            spawn(function()
                while wait(5) do
                    if autoIsland then
                        local _, islandPos = GetLevelInfo()
                        pcall(function()
                            Player.Character.HumanoidRootPart.CFrame = islandPos
                        end)
                    end
                end
            end)

            -- üñ±Ô∏è Giao Di·ªán Ti·∫øng Vi·ªát
            Window:CreateToggle({
                Name = "üó°Ô∏è B·∫≠t/T·∫Øt T·ª± ƒê·ªông ƒê√°nh Qu√°i",
                CurrentValue = false,
                Callback = function(Value)
                    autoFarm = Value
                end,
            })

            Window:CreateToggle({
                Name = "üß≠ B·∫≠t/T·∫Øt T·ª± ƒê·ªông ƒê·ªïi ƒê·∫£o Theo C·∫•p",
                CurrentValue = false,
                Callback = function(Value)
                    autoIsland = Value
                end,
            })

            Window:CreateSlider({
                Name = "‚öôÔ∏è ƒêi·ªÅu Ch·ªânh T·ªëc ƒê·ªô ƒê√°nh (gi√¢y)",
                Range = {0.1, 2},
                Increment = 0.1,
                Suffix = "gi√¢y",
                CurrentValue = 1,
                Callback = function(Value)
                    delayDanh = Value
                end,
            })

        else
            Rayfield:Notify({
                Title = "‚ùå Sai Key!",
                Content = "Vui l√≤ng nh·∫≠p ƒë√∫ng key: buituanan",
                Duration = 5,
            })
        end
    end
})
